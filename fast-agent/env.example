# Canvas API credentials
# WARNING: This token is highly sensitive - it gives full access to your Canvas account
# Do NOT share, commit, or expose this token in any way
CANVAS_API_TOKEN=your_api_token_here
CANVAS_BASE_URL=https://your-institution.instructure.com

# Google API key for Gemini model
GOOGLE_API_KEY=your_google_api_key_here
# Optional model name (default is "gemini-pro")
MODEL_NAME=gemini-pro

# Server settings
PORT=8000
HOST=localhost
MCP_SERVER_PORT=3001 